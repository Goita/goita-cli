    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "goita game",
            "program": "${workspaceRoot}/lib/cli-game.js",
            "args": [
                "game",
                "-i", "",
                "-s", "0,0",
                "-p", "1",
                "-G"
            ],
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "outFiles": [
                "${workspaceRoot}/lib/**/*.js"
                ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "goita solve [history]",
            "program": "${workspaceRoot}/bin/goita",
            "args": [
                "solve",
                "12345678,12345679,11112345,11112345,s1,113,2p,3p,431,1p,2p,315,4p,1p,2p,343,4p,1p,2p,321,4p,1p,213,3p,4p,1p,245,3p"
            ],
            "cwd": "${workspaceRoot}",
            "outFiles": [
                "${workspaceRoot}/lib/**/*.js"
                ]
        }
    ]
}
