    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "goita game",
            "program": "${workspaceRoot}/lib/cli-game.js",
            "args": [
                "game",
                "-i", "",
                "-s", "0,0",
                "-p", "1"
            ],
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "outFiles": [
                "${workspaceRoot}/lib/**/*.js"
                ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "goita solve [history]",
            "program": "${workspaceRoot}/bin/goita",
            "args": [
                "solve", 
                "12667789,12345543,11112345,11112345,s1,116,2p,3p,4p,126,2p,3p,4p"
            ],
            "cwd": "${workspaceRoot}",
            "outFiles": [
                "${workspaceRoot}/lib/**/*.js"
                ]
        }
    ]
}