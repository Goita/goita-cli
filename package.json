{
  "name": "goita-cli",
  "version": "0.0.1",
  "private": false,
  "description": "goita command line tool",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Goita/goita-cli"
  },
  "author": "yoskeoka",
  "keywords": [
    "goita"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "goita": "./bin/goita"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty -w",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive test/**/*-spec.ts",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "dependencies": {
    "@types/cli-color": "^0.3.29",
    "@types/commander": "^2.0.0",
    "cli-color": "^1.1.0",
    "commander": "^2.9.0",
    "goita-ai-sample-js": "file:../goita-ai-sample-js",
    "goita-core-js": "file:../goita-core-js"
  },
  "devDependencies": {
    "@types/chai": "^3.0.0",
    "@types/mocha": "^2.0.0",
    "@types/node": "6.0.31",
    "chai": "^3.0.0",
    "mocha": "^3.0.0",
    "rimraf": "^2.0.0",
    "ts-node": "^1.0.0",
    "tslint": "^4.0.0",
    "typescript": "^2.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  }
